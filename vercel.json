{
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/api/areas/(.*)",
      "dest": "/api/areas/$1"
    },
    {
      "src": "/api/categorias/(.*)",
      "dest": "/api/categorias/$1"
    },
    {
      "src": "/api/productos/(.*)",
      "dest": "/api/productos/$1"
    },
    {
      "src": "/api/mesas/(.*)",
      "dest": "/api/mesas/$1"
    },
    {
      "src": "/[^.]+",
      "dest": "/"
    }
  ]
}
